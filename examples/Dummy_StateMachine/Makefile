CXX=g++
CXXFLAGS=-O3 -Wall -Wextra -I.
LDFLAGS=
MAKE=make
LIBS=-lrt

COMPILE=$(CXX) $(CXXFLAGS) -c -o $@ $<
LINK=$(CXX) $(LDFLAGS) -o $@ $^ $(LIBS)

.SUFFIXES:
.SUFFIXES:	.cc .hh .o

SOURCES	:= $(wildcard *.cc)
OBJECTS	:= $(SOURCES:%.cc=%.o)
BINARY	:= Test_FSM.out

d :=	.

dir	:=	$(d)/Test_FSM
include	$(dir)/Rules.mk

%.o:	%.cc
		$(COMPILE)

$(BINARY):	$(OBJECTS)
		$(LINK)

.PHONY:	all
all:	$(SOURCES) $(OBJECTS) $(BINARY)

.PHONY:	clean
clean:
	rm -f $(OBJECTS) $(BINARY)

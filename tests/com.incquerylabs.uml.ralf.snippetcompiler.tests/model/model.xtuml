      <relations xsi:type="xtuml:XTAssociation" name="R1_pong" unique="true" lowerBound="1" upperBound="1" opposite="//@entities.0/@entities.0/@relations.0" source="//@entities.0/@entities.1" target="//@entities.0/@entities.0"/>
      <events xsi:type="xtuml:XTClassEvent" name="pong_s"/>
